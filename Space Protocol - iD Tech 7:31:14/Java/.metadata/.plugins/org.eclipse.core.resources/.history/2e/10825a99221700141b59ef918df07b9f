
public class Animal {

	String type;
	String name;
	String color;
	int age;
	int topSpeed;
	int calories;
	int distanceTraveling;
	int caloriesBurned;
	
	public Animal (String t, String n, String co, int a, int tS, int ca){
		type = t;
		name = n;
		color = co;
		age = a;
		topSpeed = tS;
		calories = ca;
	}
	
	public void travel (int dT){
		distanceTraveling = dT;
		
	}
	
}
