
public class RunMethods {

	public static void sayHello() {
		
		System.out.println("Hello!");
		
	}
	
	public static void print (String text) {
		
		System.out.println(text);
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
			sayHello();
			sayHello();
			
			print("Java says;");
			sayHello();
		
	}

}
